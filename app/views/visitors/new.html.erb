<h1>Sign Up!</h1>

<%= render partial: "layouts/errors", locals: {object: @visitor} %>

<%= form_for(@visitor, url:'/signup') do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br><br><br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br><br><br>
    <%= f.submit "Sign Up" %>
<% end %>