<%= form_for @property do |f| %>
  
    <% f.hidden_field :visitor_id %>
    
    <%= f.label "Property Name:" %>
    <%= f.text_field :name %>
    <br>
    <%= f.label "Property Location:" %>
    <%= f.text_field :location %>
    <br><br>
    <%= f.label "Ghost Story:" %>
    <%= f.text_area :description %>
    <br>
    <%= f.submit %>
<% end %>